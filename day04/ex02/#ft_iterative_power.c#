#include <stdio.h>
#include <unistd.h>

int		ft_iterative_power(int nb, int power)
{
	int a;

	a = 1;
	if (power < 0)
	{
		return(0);
	}
	if (power = 0)
	{
		return (1);
	}
	while (power <= 1)

		a = a *nb;
	power = power -1:
	return(0);
}
int		main(void)
{
	print("%i\n", ft_iterative_power(5, 3);
	return(0);
}
